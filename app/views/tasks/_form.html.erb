<% case type %>
<% when 'AskOrderTask' %>
  <%= render 'tasks/ask_order_task_form', collection_id: collection_id, identifier: identifier %>
<% when 'AuctionOrderTask' %>
  <%= render 'tasks/auction_order_task_form', collection_id: collection_id, identifier: identifier %>
<% when 'WithdrawTask' %>
  <%= render 'tasks/withdraw_task_form', collection_id: collection_id, identifier: identifier %>
<% when 'DepositTask' %>
  <%= render 'tasks/deposit_task_form', collection_id: collection_id, identifier: identifier %>
<% when 'CancelOrderTask' %>
  <%= render 'tasks/cancel_order_task_form', collection_id: collection_id, identifier: identifier, order_id: order_id %>
<% when 'FillOrderTask' %>
  <%= render 'tasks/fill_order_task_form', collection_id: collection_id, identifier: identifier, order_id: order_id %>
<% end %>
