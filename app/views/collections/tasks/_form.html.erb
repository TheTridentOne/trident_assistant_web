<% case type %>
<% when 'AskOrderTask' %>
  <%= render 'collections/ask_order_tasks/form', identifiers: identifiers %>
<% when 'AuctionOrderTask' %>
  <%= render 'collections/auction_order_tasks/form', identifiers: identifiers %>
<% when 'WithdrawTask' %>
  <%= render 'collections/withdraw_tasks/form', identifiers: identifiers %>
<% when 'TransferTask' %>
  <%= render 'collections/transfer_tasks/form', identifiers: identifiers %>
<% when 'DepositTask' %>
  <%= render 'collections/deposit_tasks/form', identifiers: identifiers %>
<% when 'CancelOrderTask' %>
  <%= render 'collections/cancel_order_tasks/form', order_ids: order_ids %>
<% when 'FillOrderTask' %>
  <%= render 'collections/fill_order_tasks/form', order_ids: order_ids %>
<% end %>
